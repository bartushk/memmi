{
  "name": "memmi-client-side",
  "version": "1.0.0",
  "description": "Client side code for the memmi project.",
  "keywords": [
    "react",
    "redux",
    "minimal",
    "starter kit",
    "boilerplate"
  ],
  "main": "index.js",
  "homepage": "https://github.com/bartushk/memmi",
  "repository": {
    "type": "git",
    "url": "https://github.com/bartushk/memmi"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --history-api-fallback --host 0.0.0.0",
    "build-dev": "webpack --config webpack.dev.config.js",
    "build-prod": "webpack -p --config webpack.prod.config.js",
    "test": "babel-tape-runner ./test/**/*.js | tap-spec",
    "test-watch": "npm test -- --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "transform-regenerator",
      "transform-runtime"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "babel-preset-stage-0": "^6.16.0",
    "flat-combine-reducers": "^0.1.2",
    "lodash": "^4.16.4",
    "material-ui": "^0.16.7",
    "protobufjs": "^6.4.6",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.5.2",
    "redux-form": "^6.0.2",
    "redux-saga": "^0.11.1",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "babel-tape-runner": "^2.0.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.8.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "image-webpack-loader": "^3.0.0",
    "node-sass": "^3.10.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  }
}
